    <servlet>
        <servlet-name>registrarConsumoZ</servlet-name>
        <servlet-class>Servlet.registrarConsumoZ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registrarUsuario</servlet-name>
        <url-pattern>/registrarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerificarUsuario</servlet-name>
        <url-pattern>/VerificarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrarConsumo</servlet-name>
        <url-pattern>/registrarConsumo</url-pattern>
