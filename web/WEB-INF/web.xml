        <servlet-name>borrarC</servlet-name>
        <url-pattern>/borrarC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>actualizarUsuario</servlet-name>
        <url-pattern>/actualizarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrarConsumoY</servlet-name>
        <url-pattern>/registrarConsumoY</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>borrarA</servlet-name>
        <url-pattern>/borrarA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrarConsumoZ</servlet-name>
        <url-pattern>/registrarConsumoZ</url-pattern>
