        <servlet-class>Servlet.registrarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerificarUsuario</servlet-name>
        <servlet-class>Servlet.VerificarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrarConsumo</servlet-name>
        <servlet-class>Servlet.registrarConsumo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrarConsumoX</servlet-name>
